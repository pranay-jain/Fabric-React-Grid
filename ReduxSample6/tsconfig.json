{
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": true,
    "noEmitOnError": true,
    "removeComments": false,
    "experimentalDecorators": true,
    "sourceMap": true,
    "module": "commonjs",
    "target": "es2015",
    "jsx": "react",
    "lib": [
      "es2015",
      "dom"
    ]
  },
  "exclude": [
    "node_modules",
    "wwwroot"
  ],
  "include": [
    "./src/**/*"
  ]
}
